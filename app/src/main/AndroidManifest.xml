<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.football.match">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/fmlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.fourth.Classes.Pahina"/>
        <activity
            android:name="com.example.fourth.Classes.Liga"
            android:parentActivityName="com.example.fourth.Classes.Bansa" />
        <activity
            android:name="com.example.fourth.Classes.Bansa"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.fourth.Classes.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.fourth.Classes.Standings"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.fourth.Classes.Score"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.fourth.Classes.Splash"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>